{
    "name": "javadoc",
    "version": "1.1.0",
    "javadoc-js": "dist/javadoc.js",
    "javadoc-css": "dist/stylesheet.css",
    "scripts": {
        "build": "parcel build"
    },
    "browserslist": "> 0.5%, last 2 versions, not dead, not ie > 0, chrome >= 57",
    "targets": {
        "javadoc-js": {
            "source": "src/index.js",
            "context": "browser",
            "isLibrary": false,
            "includeNodeModules": true
        },
        "javadoc-css": {
            "source": "src/custom_style.css"
        }
    },
    "devDependencies": {
        "@parcel/optimizer-data-url": "2.5.0",
        "@parcel/reporter-bundle-analyzer": "^2.5.0",
        "@parcel/transformer-inline-string": "2.5.0",
        "buffer": "^6.0.3",
        "jquery": "3.5.1",
        "parcel": "^2.5.0",
        "process": "^0.11.10"
    },
    "dependencies": {
        "tippy.js": "^6.3.7"
    }
}
